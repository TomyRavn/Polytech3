import java.util.Scanner;

public class Ex01 {

	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);		
		System.out.println("정수를 입력 :");
		while(true) {
		int num1 =s.nextInt();
		
		if(num1>5)
			System.out.println("정수를 다시 입력 :");
			break;
		
		for(int j=1; j<=num1; j++) {   
			for(int i=1; i<=j; i++)			
				System.out.print(i);
				System.out.println();
		
		}
		
		
		

	}

}
}
