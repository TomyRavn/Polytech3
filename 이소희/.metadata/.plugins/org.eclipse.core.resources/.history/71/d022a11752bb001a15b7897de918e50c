package ex2;

abstract public class GoodClac extends Calculator{

	@Override
	public int add(int a, int b) {
		return a + b;
	}

	@Override
	public int substract(int a, int b) {
		return a - b;
	}

	@Override
	public int[] average(int[] a) {
		return a;
	}
	
	void setValue(int a, int b, double c) {
		this.a = a;
		this.b = b;
		this.c = c;
	}
	
	abstract int calculate();


}
