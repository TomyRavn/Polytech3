
public class Ex05 {

	public static void main(String[] args) {
		int temp = 0;
		
		int[] lotto = new int[6];
		for (int i = 0; i < lotto.length; i++) {
			
		
		lotto[i]= (int) Math.random() * 45 + 1;
		}
		
		
		for(int i=0; i<9999999; i++){
		            temp = (int)(Math.random() * 45 + 1);}
		            if(lotto.length == 0){
		               int m = temp;
		            }
		            else {
		                int count = 0;
		                for(int j=0; j<lotto.length; j++){
		                
		                    if(lotto[j] == temp){
		                    count = count+1;
		                    }          
		                    }
		                    if (count == 0){
		                   int m = temp;
		            
		                        }
		                        }
		                    if(lotto.length == 6){
		                    break;
		                            }
		                            }
		        System.out.println(lotto);

	}

}
