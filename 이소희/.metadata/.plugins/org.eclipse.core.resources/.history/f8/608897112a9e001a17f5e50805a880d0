import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		

		int add = test(10, 80);
		System.out.printf("합의 결과: %d\n", add);
		
		
	}
		
	
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		public static int test(int x, int y) {
			return x + y;
	}

}


