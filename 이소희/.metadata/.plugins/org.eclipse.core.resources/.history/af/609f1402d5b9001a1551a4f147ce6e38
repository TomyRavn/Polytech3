package overring;

class CPoint {
	int x, y;
	

	public void set(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	
		public void showCPoint() {
			System.out.println(x +"," + y);
		}
			public void main(String[] args){
				CPoint a, b;

				a = new CPoint(2, 3);
				b = new CColorPoint(3, 4, "red");
				a.show();
				b.show();
				System.out.println(a);
				System.out.println(b);
				}
			}
}







