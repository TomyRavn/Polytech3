package overring;

public class CPoint {
	CPoint a, b;
	
		public void main(String[] args){
			
			protected void showPoint() {
			a = new CPoint(2, 3);
			b = new CColorPoint(3, 4, "red");
			a.show();
			b.show();
			System.out.println(a);
			System.out.println(b);
			}
}







