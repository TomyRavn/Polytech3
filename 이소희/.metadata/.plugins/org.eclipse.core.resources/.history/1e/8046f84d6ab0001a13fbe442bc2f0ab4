<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
    <%String nickParam = request.getParameter("nick");
    String nickSession = (String)session.getAttribute("userNick");
    if (nickParam != null && nickSession == null) {
    	session.setAttribute("userNick", nickParam);
    }
    %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	[회원관리]
	[게시판]
	[학생관리]
	
<!-- 	로그인하지 않은 경우 -->
	
	<a href='${pageContext.request.contextPath}/member/login.do?memId=${vo.memId}'>[로그인]</a>
	
<!-- 	로그인한 경우 -->
	<c:if test="${userNick != null}">
	<h1>${userNick}님,안녕하세요.</h1>
	</c:if>
	<a href='${pageContext.request.contextPath}/member/logout.do?memId=${vo.memId}'>[로그아웃]</a>
	
	
</body>
</html>