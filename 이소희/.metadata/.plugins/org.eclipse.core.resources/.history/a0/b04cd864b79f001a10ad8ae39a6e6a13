import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {
		// 난수를 5개 만들어 값을 저장하고, 최대값 찾기
		double max = 0;
	
		for(int i=0; i<5; i++) {
			double num = Math.random();
			System.out.printf("%d번째 난수:",i+1);
			
			max = num;
			if (num>max)
			max=num;
		}
		System.out.printf("최대값: %d", max);
		

	}

}
