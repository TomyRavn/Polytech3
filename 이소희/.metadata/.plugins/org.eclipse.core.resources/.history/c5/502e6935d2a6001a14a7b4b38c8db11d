package kr.ac.kopo.exapp;

public class Member {


		private String id;
		private String name;
		private int point;
		
		public void setId(String a) {
			this.id = a;
		}
		public void setName(String b) {
			this.name = b;
		}
		public void setPoint(int c) {
			this.point = c;
		}
		
		
		public String getId() {
			return this.id;
			
		}
		public String getName() {
			return this.name;
			
		}

			public int getPoint() {
			return this.point;

			}
}
