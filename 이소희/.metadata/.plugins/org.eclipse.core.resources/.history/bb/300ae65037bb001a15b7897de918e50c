package ex;

import java.util.Scanner;

public class Sub extends Calc{
	Scanner sc = new Scanner(System.in);
	
	@Override
	public void cal() {
		System.out.println("1.덧셈 2.뺄셈 3.나눗셈 4.곱셈 >> ");
		int a = sc.nextInt();
		System.out.println("숫자를 입력하시오.");
		int b = sc.nextInt();
		System.out.println("숫자를 입력하시오.");
		int c = sc.nextInt();	
	}

}
