
public class Ex05 {

	public static void main(String[] args) {
		var a = Math.random() * 45 + 1
		        lotto=[]

		        for(var i=0; i<9999999; i++){
		            var temp = parseInt(Math.random() * 45 + 1)
		            if(lotto.length == 0){
		                lotto.push(temp)
		            }
		            else {
		                var count = 0
		                for(var j=0; j<lotto.length; j++){
		                
		                    if(lotto[j] == temp){
		                    count = count+1
		                    }          
		                    }
		                    if (count == 0){
		                    lotto.push(temp)
		            
		                        }
		                        }
		                    if(lotto.length == 6){
		                    break
		                            }
		                            }
		        alert(lotto)

	}

}
