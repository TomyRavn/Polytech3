package overring;

class CPoint {
	CPoint a, b;
	
	public CPoint(int i, int j) {
		// TODO Auto-generated constructor stub
	}


	public void set(int a, int b) {
		this.a = a;
		this.b = b;
	}
	
	
		public void showPoint() {
			a = new CPoint(2, 3);
			b = new CColorPoint(3, 4, "red");
			a.show();
			b.show();
			System.out.println(a);
			System.out.println(b);
			}
}







