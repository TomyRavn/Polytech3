package kr.ac.kopo.exapp;

public class Food {
	private String name;
	private int price;
	
	
	
	public void setName(String n) {
		this.name = n;
		
		
	}
	public void setPrice(int p) {
	this.price = p;
	
}

	public String getName() {
	return this.name;
	
}

	public int getPrice() {
	return this.price;
}
}

